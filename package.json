{
  "name": "pagina",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^17.2.1",
    "mysql2": "^3.14.3"
  },
  "scripts": {
    "start": "npm run dev --prefix ocr-visual",
    "start:backend": "node --env-file=server/.env server/server.js",
    "frontend": "npm run dev --prefix ocr-visual",
    "dev": "concurrently --kill-others --prefix-colors \"auto\" \"node --env-file=server/.env server/server.js\" \"npm run dev --prefix ocr-visual\"",
    "install:all": "cd ocr-visual && npm install && cd ../server && npm install && pip install -r requirements.txt"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}